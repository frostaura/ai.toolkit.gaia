{
    "servers": {
        "playwright-mcp": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@playwright/mcp@latest",
                "--caps=vision"
            ]
        },
        "gaia": {
            "type": "stdio",
            "command": "dotnet",
            "args": [
                "run",
                "--project",
                ".github/mcps/gaia/src/fa.mcp.gaia/fa.mcp.gaia.csproj"
            ]
        },
        "chakra-ui": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@chakra-ui/react-mcp"
            ]
        }
    },
    "inputs": []
}
